# csv 파일을 읽은 뒤에, 데이터 확인하기

import csv
f = open('seoul_2000.csv', encoding='cp949')
data = csv.reader(f, delimiter=',')

for i in data :
  print(i)

f.close()


# 헤더 저장하기 

import csv
f = open('seoul_2000.csv', encoding='cp949')
data = csv.reader(f, delimiter=',')

header = next(data)
print(header)

f.close()


